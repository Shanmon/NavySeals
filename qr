CREATE TABLE IF NOT EXISTS FORAS.LF_COMBINED
{
	PROPERTY_WID BIGINT GENERATED ALWAYS AS IDENTITY,
	PROPERTY_ADDRESS	varchar(9999),
	City	varchar(50),
	LOCATION	varchar(100),
	BUILDING_NAME varchar(9999),
--	BUILDING_NAME_CLEANED varchar(9999),
--	NORM_BUILDING_NAME_CLEANED	varchar(9999),
	DW_SESSION_NAME varchar(50),
	DW_INSERT_DTS TIMESTAMP,
	DW_UPDATE_DTS TIMESTAMP,
	CONSTRAINT property_pkey PRIMARY KEY (PROPERTY_WID)
);
